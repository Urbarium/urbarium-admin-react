{
  "name": "urbarium-admin",
  "version": "1.0.1",
  "description": "Admin Site for Urbarium.org",
  "homepage": "https://urbarium-org.firebaseapp.com",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "lint": "./node_modules/eslint/bin/eslint.js src/**/*.js",
    "build": "react-scripts build",
    "deploy": "$(npm bin)/now",
    "test": "react-scripts test --env=jsdom",
    "serve": "$(npm bin)/serve -l 3000 build",
    "cypress": "$(npm bin)/cypress run",
    "eject": "react-scripts eject",
    "surge": "bash surge_login.sh && surge --project ./build/ --domain atlaskit-starter.surge.sh",
    "now-build": "npm run build",
    "storybook": "start-storybook --port 9009",
    "chromatic": "chromatic test",
    "snyk-protect": "$(npm bin)/snyk protect"
  },
  "keywords": [],
  "author": "Urbarium",
  "license": "ISC",
  "dependencies": {
    "@atlaskit/avatar": "^17.1.9",
    "@atlaskit/button": "^13.3.9",
    "@atlaskit/css-reset": "^5.0.10",
    "@atlaskit/drawer": "^5.3.5",
    "@atlaskit/form": "^7.2.0",
    "@atlaskit/icon": "^20.1.0",
    "@atlaskit/inline-message": "^10.1.5",
    "@atlaskit/modal-dialog": "^10.5.5",
    "@atlaskit/navigation-next": "^8.0.3",
    "@atlaskit/page": "^11.0.12",
    "@atlaskit/portal": "^3.1.6",
    "@atlaskit/section-message": "^4.1.7",
    "@atlaskit/textfield": "^3.1.9",
    "@atlaskit/theme": "^9.5.2",
    "@sentry/browser": "^5.15.4",
    "connected-react-router": "^6.8.0",
    "dotenv": "^8.2.0",
    "firebase": "^8.0.1",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "logrocket": "^1.0.7",
    "logrocket-react": "^4.0.1",
    "math": "^0.0.3",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-flexview": "^4.0.4",
    "react-loading": "^2.0.3",
    "react-moment": "^0.9.7",
    "react-redux": "^7.2.0",
    "react-redux-firebase": "^3.3.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-auth-wrapper": "^3.0.0",
    "redux-firestore": "^0.13.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "snyk": "^1.290.1",
    "styled-components": "^5.1.0",
    "victory": "^34.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@storybook/react": "^5.3.18",
    "babel-eslint": "^10.1.0",
    "cypress": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "now": "^17.1.1",
    "npm-start-command": "^1.0.0",
    "patch-package": "^6.2.2",
    "react-scripts": "^3.4.1",
    "redux-devtools-extension": "^2.13.8",
    "serve": "^11.3.0",
    "storybook-chromatic": "^3.5.2",
    "webpack": "^4.42.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "snyk": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && CI=true npm test",
      "pre-push": "npm run build"
    }
  }
}
