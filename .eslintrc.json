{
  "plugins": ["jest"],
  "extends": ["airbnb","plugin:jest/recommended"],
  "parser": "babel-eslint",
  "rules": {
    "indent": ["warn", 2],
    "react/jsx-indent": ["warn", 2],
    "max-len": [1, 200, 2],
    "quote-props": [1, "consistent-as-needed"],
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "import/no-extraneous-dependencies": 0,
    "quotes": 0
  },
  "env": {
    "jest/globals": true
  }
}
